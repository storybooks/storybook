{
  "extends": "./tsconfig.json",
  "include": [
    ".*.js",
    "**/*.ts",
    "**/*.tsx",
    "**/*.json",
    "*.json",
    "tsconfig.json",
    "**/tsconfig.json",
    "**/.eslintrc.js",
    "**/.storybook/*.*",
    "**/.storybook/**/*.*",
    "**/*.html",
    "**/*.js",
    "**/.*.js",
    "**/*.jsx"
  ],
  "exclude": [
    "**/dist",
    "node_modules",
    "**/node_modules",
    "**/*.bundle.js",
    "**/*.js.map",
    "**/*.d.ts"
  ]
}
